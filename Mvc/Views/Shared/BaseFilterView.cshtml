@model TodoServices.Shared.Dto.BaseFilter

<div class="form-group">
    <label for="From" class="control-label">@Localizer["Create date from"]</label>
    <input asp-for="From" class="form-control" />
    <span asp-validation-for="From" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="To" class="control-label">@Localizer["Create date to"]</label>
    <input asp-for="To" class="form-control" />
    <span asp-validation-for="To" class="text-danger"></span>
</div>
<button type="submit" class="btn btn-primary"><i class="fa fa-filter"></i> @Localizer["Find"]</button>

